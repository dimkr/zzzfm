#: ../src/cust-dialog.c:3410
#, c-format
msgid ""
"\n"
"EXAMPLE SCRIPT:\n"
msgstr ""
"\n"
"SCENRAŽIO PAVYZDYS:\n"

#. Here QUOTED means between ` ` - like `QUOTED`
#: ../src/cust-dialog.c:3411
#, c-format
msgid ""
"    #!%s\n"
"    # This script shows a Yes/No dialog\n"
"    # Use QUOTED eval to read variables output by zzzFM Dialog:\n"
"    eval \"`zzzfm -g --label \"Are you sure?\" --button yes --button no`\"\n"
"    if [[ \"$dialog_pressed\" == \"button1\" ]]; then\n"
"        echo \"User pressed Yes - take some action\"\n"
"    else\n"
"        echo \"User did NOT press Yes - abort\"\n"
"    fi\n"
msgstr ""
"    #!%s\n"
"    # Šis scenarijus rodo dialogo langą Taip/Ne\n"
"    # Naudokite QUOTED eval, kad perskaitytumėte kintamuosius, kuriuos išveda by zzzFM Dialogas:\n"
"    eval \"`zzzfm -g --label \"Ar tu tuo tikras?\" --button taip --button ne`\"\n"
"    if [[ \"$dialog_pressed\" == \"button1\" ]]; then\n"
"        echo \"Vartotojas paspaudė Taip – u200bu200bimkitės veiksmų\"\n"
"    else\n"
"        echo \"Vartotojas NEPASPUSĖ Taip – u200bu200bnutraukti\"\n"
"    fi\n"

