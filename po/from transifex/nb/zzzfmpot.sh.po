#, c-format
msgid ""
"    zzzfm -g --label \"Enter some text and press Enter:\" \\\n"
"               --input \"\" set label2 %%v -- echo '# %%n = %%v' \\\n"
"               --label \\\n"
"               --button ok\n"
msgstr ""
"    zzzfm -g --label \"Skriv inn litt tekst og trykk Enter:\" \\\n"
