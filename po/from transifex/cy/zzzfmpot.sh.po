"    # Mae'r sgript hon yn dangos deialog Ie / Na\n"
"    # Defnyddiwch werthusiad QUOTED i ddarllen allbwn newidynnau erbyn zzzFM Deialog:\n"
"    eval \"`zzzfm -g --label \"Wyt ti'n siwr?\" --button ie --button na`\"\n"
"    if [[ \"$dialog_pressed\" == \"button1\" ]]; then\n"
"        echo \"Pwysodd y defnyddiwr Ydw - cymerwch gamau\"\n"
"    else\n"
"        echo \"NI wnaeth y defnyddiwr bwyso Do - erthylu\"\n"
"    fi\n"

