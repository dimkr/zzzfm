"    eval \"`zzzfm -g --label \"តើu200bអ្នកu200bប្រាកដu200bឬu200bអត់?\" --button បាទ --button ទេ`\"\n"
"    if [[ \"$dialog_pressed\" == \"button1\" ]]; then\n"
"        echo \"អ្នកប្រើប្រាស់ចុច Yes - ធ្វើសកម្មភាពខ្លះ\"\n"
"    else\n"
"        echo \"អ្នកប្រើប្រាស់មិនបានចុច Yes - abort\"\n"
"    fi\n"

